<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="https://www.dita-ot.org/project">
  
  <context name="Oxygen XML Author" id="oxygenAuthor">
    <input href="../UserManual.ditamap"/>
    <profile>
      <ditaval href="../ditaval/author-sa.ditaval"/>
    </profile>
  </context>
  <context name="Oxygen XML Editor" id="oxygenEditor">
    <input href="../UserManual.ditamap"/>
    <profile>
      <ditaval href="../ditaval/editor-sa.ditaval"/>
    </profile>
  </context>
  <context name="Oxygen XML Developer" id="oxygenDeveloper">
    <input href="../UserManual.ditamap"/>
    <profile>
      <ditaval href="../ditaval/developer-sa.ditaval"/>
    </profile>
  </context>
  
  <deliverable name="Oxygen XML Author to HTML5" id="oxygenXmlAuthorToHTML5">
    <context idref="oxygenAuthor"/>
    <output href="./site-author-html5"/>
