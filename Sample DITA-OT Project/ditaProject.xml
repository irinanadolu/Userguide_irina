  <deliverable name="Oxygen XML Editor to HTML5" id="oxygenXmlEditorToHTML5">
    <context idref="oxygenEditor"/>
    <output href="./site-editor-html5"/>
    <publication transtype="html5"/>
  </deliverable>
  <deliverable name="Oxygen XML Developer to HTML5" id="oxygenXmlDeveloperToHTML5">
    <context idref="oxygenDeveloper"/>
    <output href="./site-developer-html5"/>
    <publication transtype="html5"/>
  </deliverable>
  
  <deliverable name="Oxygen XML Author to PDF" id="oxygenXmlAuthorToPDF">
    <context idref="oxygenAuthor"/>
    <output href="./site-author-pdf"/>
    <publication transtype="pdf"/>
  </deliverable>
  <deliverable name="Oxygen XML Editor to PDF" id="oxygenXmlEditorToPDF">
    <context idref="oxygenEditor"/>
    <output href="./site-editor-pdf"/>
    <publication transtype="pdf"/>
  </deliverable>
  <deliverable name="Oxygen XML Developer to PDF" id="oxygenXmlDeveloperToPDF">
